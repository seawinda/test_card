{
  "name": "Card",
  "version": "1.0.0",
  "private": true,
  "description": "Карточка для теста",
  "devDependencies": {
    "autoprefixer": "8.3.x",
    "browser-sync": "2.23.x",
    "gulp": "3.9.x",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-sass": "4.0.x",
    "@htmlacademy/editorconfig-cli": "1.x",
    "stylelint": "9.2.x",
    "stylelint-config-htmlacademy": "0.1.x",
    "gulp-typograf": "^3.1.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp style",
    "start": "gulp serve",
    "typo": "gulp typograf"
  },
  "browserslist": [
    "last 10 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.11"
  },
  "dependencies": {
    "gh-pages": "^2.1.1",
    "npm": "^6.7.0",
    "path": "^0.12.7",
    "svgo": "^1.3.2",
    "tipograf": "^2.0.0",
    "typograf": "^6.7.2"
  }
}
